{
	"Ducklings":[
		{
			"Label":"junk",
			"Description":"'j <path>' moves <path> to .junk",
			"Commands":[
				"ls -d .junk > /dev/null 2> /dev/null || mkdir .junk",
				"mv $1 $path/.junk"
			],
			"Aliases":["j"]
		},
		{
			"Label":"free",
			"Description":"Empty the .junk directory",
			"Commands":[
				"rm -r .junk/*"
			],
			"Aliases":["f"]
		}
	]
}